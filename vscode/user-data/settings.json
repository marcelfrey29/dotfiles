{
    // Telemetry
    "telemetry.telemetryLevel": "off",
    "redhat.telemetry.enabled": false,
    "workbench.enableExperiments": false,
    "aws.telemetry": false,
    "continue.telemetryEnabled": false,
    // Window
    "window.commandCenter": true,
    // Activity Bar
    "workbench.activityBar.location": "top",
    // Workbench
    "workbench.tree.renderIndentGuides": "always",
    "workbench.tree.indent": 24,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.hover.delay": 250,
    "workbench.list.smoothScrolling": true,
    "workbench.view.alwaysShowHeaderActions": true,
    "workbench.preferredDarkColorTheme": "GitHub Dark Dimmed",
    "workbench.preferredLightColorTheme": "GitHub Light",
    "workbench.colorTheme": "GitHub Dark Dimmed",
    "workbench.iconTheme": "symbols",
    "workbench.panel.showLabels": false,
    "workbench.editor.closeOnFileDelete": true,
    "workbench.editor.enablePreview": false,
    "workbench.editor.splitInGroupLayout": "vertical",
    "workbench.editor.openPositioning": "last",
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 8,
    "workbench.editor.limit.perEditorGroup": true,
    "workbench.colorCustomizations": {
        "[GitHub Dark Dimmed]": {
            "focusBorder": "#00000000",
            "titleBar.border": "#00000000",
            "activityBar.border": "#00000000",
            "editorWidget.border": "#00000000",
            "activityBarBadge.background": "#f69d50",
            "activityBarBadge.foreground": "#000000",
            "scrollbarSlider.background": "#ADBAC725",
            "editorCursor.foreground": "#6cb6ff",
            "editorLineNumber.activeForeground": "#6cb6ff",
            "editor.foldBackground": "#10151C",
            "editor.lineHighlightBackground": "#30353C",
            "editorGutter.addedBackground": "#6BC46DBF",
            "editorGutter.modifiedBackground": "#6cb6ff",
            "editorGutter.deletedBackground": "#F47067",
            "badge.background": "#f69d50",
            "badge.foreground": "#000000",
            "gitDecoration.modifiedResourceForeground": "#6cb6ff",
            "icon.foreground": "#ADBAC7",
            "symbolIcon.keywordForeground": "#F47067",
            "symbolIcon.methodForeground": "#6CB6FF",
            "symbolIcon.functionForeground": "#6CB6FF",
            "symbolIcon.constructorForeground": "#6CB6FF",
            "symbolIcon.enumeratorMemberForeground": "#6CB6FF",
            "symbolIcon.classForeground": "#F69D50",
            "symbolIcon.interfaceForeground": "#F69D50",
            "symbolIcon.structForeground": "#F69D50",
            "symbolIcon.moduleForeground": "#F69D50",
            "symbolIcon.namespaceForeground": "#F69D50",
            "symbolIcon.packageForeground": "#F69D50",
            "symbolIcon.objectForeground": "#F69D50",
            "symbolIcon.arrayForeground": "#F69D50",
            "symbolIcon.propertyForeground": "#ADBAC7",
            "symbolIcon.fieldForeground": "#ADBAC7",
            "symbolIcon.referenceForeground": "#daaa3f",
            "symbolIcon.variableForeground": "#daaa3f",
            "symbolIcon.numberForeground": "#dcbdfb",
            "symbolIcon.stringForeground": "#6bc46d",
            "symbolIcon.textForeground": "#6bc46d",
            "button.background": "#6CB6FF",
            "button.hoverBackground": "#4C96FF"
        },
        "[GitHub Light]": {
            "focusBorder": "#00000000",
            "titleBar.border": "#00000000",
            "activityBar.border": "#00000000",
            "editorWidget.border": "#00000000",
            "activityBarBadge.background": "#0969da",
            "activityBarBadge.foreground": "#ffffff",
            "activityBar.activeBackground": "#6A737D30",
            "scrollbarSlider.background": "#6A737D50",
            "editorCursor.foreground": "#0969da",
            "editorLineNumber.activeForeground": "#0969da",
            "editor.lineHighlightBackground": "#6A737D20",
            "editorGutter.addedBackground": "#2da44e",
            "editorGutter.modifiedBackground": "#54aeff",
            "editorGutter.deletedBackground": "#ec6547",
            "badge.background": "#0969da",
            "badge.foreground": "#ffffff",
            "gitDecoration.modifiedResourceForeground": "#54aeff",
            "icon.foreground": "#6A737D",
            "symbolIcon.keywordForeground": "#ec6547",
            "symbolIcon.methodForeground": "#0969da",
            "symbolIcon.functionForeground": "#0969da",
            "symbolIcon.constructorForeground": "#0969da",
            "symbolIcon.enumeratorMemberForeground": "#0969da",
            "symbolIcon.classForeground": "#ec6547",
            "symbolIcon.interfaceForeground": "#ec6547",
            "symbolIcon.structForeground": "#ec6547",
            "symbolIcon.moduleForeground": "#ec6547",
            "symbolIcon.namespaceForeground": "#ec6547",
            "symbolIcon.packageForeground": "#ec6547",
            "symbolIcon.objectForeground": "#ec6547",
            "symbolIcon.arrayForeground": "#ec6547",
            "symbolIcon.propertyForeground": "#6A737D",
            "symbolIcon.fieldForeground": "#6A737D",
            "symbolIcon.referenceForeground": "#d4a72c",
            "symbolIcon.variableForeground": "#d4a72c",
            "symbolIcon.numberForeground": "#a475f9",
            "symbolIcon.stringForeground": "#116329",
            "symbolIcon.textForeground": "#116329",
            "button.background": "#0969da",
            "button.hoverBackground": "#54aeff",
            "tab.activeBorderTop": "#0969da",
            "activityBar.activeBorder": "#0969da",
            "panelSection.border": "#0969da",
            "panelSectionHeader.border": "#0969da",
            "terminal.tab.activeBorder": "#0969da",
            "panelSectionHeader.foreground": "#0969da"
        },
        "statusBar.background": "#00000000",
        "statusBar.noFolderBackground": "#00000000",
        "statusBar.debuggingBackground": "#00000000",
        "statusBar.foreground": "#6bc46d",
        "statusBar.debuggingForeground": "#6bc46d"
    },
    // Explorer
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 1,
    "explorer.confirmPasteNative": false,
    // Outline
    "outline.showVariables": false,
    "outline.showProperties": true,
    "[typescript]": {
        "outline.showVariables": false,
        "outline.showProperties": true
    },
    "[terraform]": {
        "outline.showArrays": false,
        "outline.showBooleans": false,
        "outline.showNumbers": false,
        "outline.showStrings": false,
        "outline.showVariables": false,
        "outline.showConstants": false
    },
    // Search
    "search.showLineNumbers": true,
    // AI / Chat / Copilot
    "chat.commandCenter.enabled": false,
    // Zen Mode
    "zenMode.hideLineNumbers": false,
    // Files
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 5000,
    // Breadcrumbs
    "breadcrumbs.enabled": false,
    "breadcrumbs.filePath": "on",
    "breadcrumbs.icons": true,
    "breadcrumbs.symbolSortOrder": "position",
    // Source Control / Git
    "scm.alwaysShowActions": true,
    "scm.diffDecorationsGutterWidth": 4,
    "scm.inputFontFamily": "editor",
    "scm.inputFontSize": 14,
    "scm.defaultViewMode": "tree",
    "git.confirmSync": false,
    "diffEditor.codeLens": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.experimental.showMoves": true,
    "diffEditor.useInlineViewWhenSpaceIsLimited": false,
    "diffEditor.hideUnchangedRegions.enabled": true,
    // Editor
    "editor.tabSize": 4,
    "editor.tabCompletion": "off",
    "editor.fontFamily": "'JetBrains Mono', 'FiraCode-Retina', Monaco, Consolas, 'Apple Color Emoji'",
    "editor.fontSize": 14,
    "editor.fontLigatures": true,
    "editor.codeLens": true,
    "editor.codeLensFontSize": 12,
    "editor.inlayHints.enabled": "on",
    "editor.wordWrap": "off",
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.hover.enabled": true,
    "editor.hover.delay": 250,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "editor.suggestOnTriggerCharacters": true,
    "editor.suggest.localityBonus": true,
    "editor.suggest.showStatusBar": true,
    "editor.acceptSuggestionOnEnter": "on",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.showFoldingControls": "always",
    "editor.linkedEditing": true,
    "editor.renderWhitespace": "none",
    "editor.renderLineHighlight": "all",
    "editor.smoothScrolling": true,
    "editor.parameterHints.enabled": true,
    "editor.parameterHints.cycle": true,
    "editor.rename.enablePreview": false,
    "editor.scrollbar.vertical": "visible",
    "editor.scrollbar.verticalScrollbarSize": 12,
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.scale": 2,
    "editor.minimap.showSlider": "always",
    "editor.minimap.maxColumn": 60,
    "editor.semanticHighlighting.enabled": true,
    "editor.bracketPairColorization.enabled": false,
    "editor.stablePeek": false,
    "editor.stickyScroll.enabled": false,
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit"
    },
    "editor.wordBasedSuggestions": "matchingDocuments",
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": false
    },
    "editor.snippetSuggestions": "top",
    "editor.quickSuggestionsDelay": 100,
    "editor.multiDocumentOccurrencesHighlight": true,
    "editor.tokenColorCustomizations": {
        "[GitHub Dark Dimmed]": {
            // "comments": "",
            // "keywords": "",
            "functions": "#6cb6ff",
            "strings": "#6bc46d",
            "numbers": "#dcbdfb",
            // "types": "",
            "variables": "#daaa3f",
            "textMateRules": [
                {
                    "name": "Lighgray (Default Text)",
                    "scope": [
                        "keyword.operator.assignment"
                    ],
                    "settings": {
                        "foreground": "#ADBAC7"
                    }
                }
            ]
        },
        "[GitHub Light]": {
            // "comments": "",
            // "keywords": "",
            "functions": "#0969da",
            "strings": "#116329",
            "numbers": "#a475f9",
            "types": "#ec6547",
            "variables": "#d4a72c",
            "textMateRules": [
                {
                    "name": "Lighgray (Default Text)",
                    "scope": [
                        "keyword.operator.assignment"
                    ],
                    "settings": {
                        "foreground": "#6A737D"
                    }
                }
            ]
        }
    },
    "editor.accessibilitySupport": "off",
    // Problems
    "problems.defaultViewMode": "table",
    "problems.showCurrentInStatus": true,
    "problems.decorations.enabled": true,
    // Debugger
    "debug.inlineValues": "on",
    "debug.showInStatusBar": "always",
    "debug.console.fontFamily": "'JetBrains Mono', 'FiraCode-Retina', Monaco, Consolas, 'Apple Color Emoji'",
    "debug.console.fontSize": 13,
    "debug.console.collapseIdenticalLines": true,
    "debug.showBreakpointsInOverviewRuler": true,
    "debug.toolBarLocation": "commandCenter",
    "debug.allowBreakpointsEverywhere": true,
    // Terminal
    "terminal.integrated.fontFamily": "'JetBrains Mono', 'FiraCode-Retina', Monaco, Consolas, 'Apple Color Emoji'",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.fontLigatures": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.scrollback": 100000,
    // Extensions
    "extensions.ignoreRecommendations": true,
    // Language - Go
    "gopls": {
        "ui.semanticTokens": true,
    },
    "[go]": {
        "editor.formatOnSave": false,
        "editor.defaultFormatter": "golang.go"
    },
    // Language - Rust
    "rust-analyzer.inlayHints.closureReturnTypeHints.enable": "always",
    "rust-analyzer.inlayHints.typeHints.hideNamedConstructor": false,
    "rust-analyzer.check.command": "clippy",
    "rust-analyzer.rustfmt.overrideCommand": [
        "leptosfmt",
        "--stdin",
        "--rustfmt"
    ],
    // Language - JavaScript
    "javascript.inlayHints.enumMemberValues.enabled": true,
    "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
    "javascript.inlayHints.parameterNames.enabled": "all",
    "javascript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": false,
    "javascript.inlayHints.parameterTypes.enabled": true,
    "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
    "javascript.inlayHints.variableTypes.enabled": true,
    "javascript.inlayHints.variableTypes.suppressWhenTypeMatchesName": false,
    // Language - TypeScript
    "typescript.implementationsCodeLens.enabled": true,
    "typescript.inlayHints.enumMemberValues.enabled": true,
    "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
    "typescript.inlayHints.parameterNames.enabled": "all",
    "typescript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": false,
    "typescript.inlayHints.parameterTypes.enabled": true,
    "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
    "typescript.inlayHints.variableTypes.enabled": true,
    "typescript.inlayHints.variableTypes.suppressWhenTypeMatchesName": false,
    // Language - XML
    "xml.codeLens.enabled": true,
    // Language - Terraform
    "terraform.codelens.referenceCount": true,
    "terraform.languageServer.enable": true,
    "terraform.experimentalFeatures.validateOnSave": true,
    // Language - Markdown
    "[markdown]": {
        "editor.wordWrap": "off" // Explicitly required...
    },
    // Icon Theme - Symbols
    "symbols.files.associations": {},
    "symbols.folders.associations": {},
    "symbols.hidesExplorerArrows": false,
    // Todo-Tree
    "todo-tree.general.showActivityBarBadge": true,
    "todo-tree.highlights.highlightDelay": 0,
    "todo-tree.highlights.useColourScheme": true,
    "todo-tree.tree.expanded": true,
    "todo-tree.tree.flat": true,
    "todo-tree.tree.showCountsInTree": true,
    // The values of the next two properties must be in order!
    // Example: `todo-tree.general.tags[0]` (here `"TODO"`) must be the first element in 
    // the `todo-tree.highlights.customHighlight` object (here: `"TODO": {}`).
    "todo-tree.general.tags": [
        "TODO",
        "BUG",
        "FIXME",
        "FIXIT"
    ],
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "icon": "check-circle",
            "iconColour": "#daaa3f",
            "gutterIcon": true,
            "foreground": "#000000",
            "background": "#daaa3f"
        },
        "BUG": {
            "icon": "x-circle",
            "iconColour": "#F47067",
            "gutterIcon": true,
            "foreground": "#000000",
            "background": "#F47067"
        },
        "FIXME": {
            "icon": "alert",
            "iconColour": "#F69D50",
            "gutterIcon": true,
            "foreground": "#000000",
            "background": "#F69D50"
        },
        "FIXIT": {
            "icon": "alert",
            "iconColour": "#F69D50",
            "gutterIcon": true,
            "foreground": "#000000",
            "background": "#F69D50"
        }
    },
    // Extensions - Vim
    "vim.useSystemClipboard": true,
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.duration": 1500,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.normal": [
        "#00000000",
        "#818b98",
    ],
    "vim.statusBarColors.insert": [
        "#00000000",
        "#6cb6ff"
    ],
    "vim.statusBarColors.visual": [
        "#00000000",
        "#6bc46d"
    ],
    "vim.statusBarColors.visualline": [
        "#00000000",
        "#6bc46d"
    ],
    "vim.statusBarColors.commandlineinprogress": [
        "#0969da",
        "#f6f8fa"
    ],
    "vim.statusBarColors.searchinprogressmode": [
        "#9a6700",
        "#f6f8fa"
    ],
    "vim.statusBarColors.surroundinputmode": [
        "#8250df",
        "#f6f8fa"
    ],
}